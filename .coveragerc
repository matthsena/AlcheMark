[run]
source = src
omit =
    */main.py
    */__init__.py
    */conftest.py
    */__pycache__/*
    */.pytest_cache/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError

[html]
directory = htmlcov 